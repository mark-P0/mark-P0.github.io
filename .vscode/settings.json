{
  "[json][jsonc][javascript][markdown][css][typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[astro]": {
    "editor.codeActionsOnSave": {
      "source.sortImports": true
    },
    "editor.tabSize": 2
  },
  "cSpell.words": ["astrojs", "tsconfigs", "astro"],
  "cSpell.allowCompoundWords": true,
  "files.associations": {
    "*.css": "tailwindcss"
  }
}
